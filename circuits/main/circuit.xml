<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE circuit PUBLIC "-//MYFUSES//DTD CIRCUIT 0.9.9 Strict//EN"
		"http://www.candango.org/publish/formats/circuit.dtd">

<circuit>

	<prefuseaction>
		<include file="actGetConf"/>
		<include file="actBuildMenu"/>
		<do action="menu"/>
	</prefuseaction>

	<fuseaction name="menu" >
		<xfa name="goToIndex" value="main.index"/>
		<xfa name="goToApplication" value="framework.application"/>
	</fuseaction>

	<fuseaction name="index" />

	<postfuseaction>
		<include file="actGetDocString"/>
		<include file="dspHeader"/>
		<include file="dspShowDocString"/>
		<include file="dspFooter"/>
	</postfuseaction>

</circuit>